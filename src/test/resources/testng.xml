<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Web Automation Suite" >
    <!-- Register ReportPortal listener -->
    <listeners>
        <listener class-name="com.epam.reportportal.testng.ReportPortalTestNGListener"/>
    </listeners>
    <parameter name="env" value="demo"/>
    <test name="Login Tests">
        <classes>
            <class name="tests.RegisterUserTest"/>
            <class name="tests.ForgotPasswordTest"/>
            <class name="tests.SearchPageTest"/>
        </classes>
    </test>
</suite>
